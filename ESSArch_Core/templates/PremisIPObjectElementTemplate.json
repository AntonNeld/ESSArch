{
  "-name": "object",
  "-namespace": "premis",
  "-children": [
    {
      "-name": "objectIdentifier",
      "-namespace": "premis",
      "-children": [
        {
          "-name": "objectIdentifierType",
          "-namespace": "premis",
          "#content": [{"var": "FIDType"}],
          "-children": []
        },
        {
          "-name": "objectIdentifierValue",
          "-namespace": "premis",
          "#content": [{"var": "FID"}],
          "-children": []
        }
      ]
    },
    {
      "-name": "objectCharacteristics",
      "-namespace": "premis",
      "-children": [
        {
          "-name": "format",
          "-namespace": "premis",
          "-children": [
            {
              "-name": "formatDesignation",
              "-namespace": "premis",
              "-children": [
                {
                  "-name": "formatName",
                  "-namespace": "premis",
                  "#content": [{"var": "FFormatName"}],
                  "-children": []
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "-name": "storage",
      "-namespace": "premis",
      "-children": [
        {
          "-name": "contentLocation",
          "-namespace": "premis",
          "-children": [
            {
              "-name": "contentLocationType",
              "-namespace": "premis",
              "#content": [{"var": "FLocationType"}],
              "-children": []
            },
            {
              "-name": "contentLocationValue",
              "-namespace": "premis",
              "#content": "file:///{{_OBJID}}.{{_IP_CONTAINER_FORMAT | lower}}",
              "-children": []
            }
          ]
        }
      ]
    }
  ],
  "-attr": [
    {
      "-name": "type",
      "-namespace": "xsi",
      "-req": "1",
      "#content": [{"text": "premis:file"}]
    }
  ]
}
